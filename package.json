{
  "name": "electron_monarch1",
  "version": "0.1.0",
  "private": true,
  "description": "control Monarch LCS ",
  "author": "Alexander Galimzyanov",
  "build": {
    "appId": "com.control.monarch-lcs"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "wait-on": "^3.3.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "react-icons": "^3.7.0",
    "animate.css": "^3.7.2",
    "react-router-dom": "^5.0.1",
    "npm-check-updates": "^3.1.20",
    "bootstrap": "^4.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "font-awesome": "^4.7.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "GENERATE_SOURCEMAP=false react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder -wl",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "prettier/react",
      "prettier/standard"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "electron": "^5.0.8",
    "electron-builder": "^21.1.5"
  }
}